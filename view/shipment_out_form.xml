<data>
  <xpath expr="/form/notebook" position="inside">
        <page string="Logistics Operator" name="algevasa_warehouse">
            <label name="not_synch_algevasa"/>
            <field name="not_synch_algevasa"/>
            <label name="algevasa_synched"/>
            <field name="algevasa_synched"/>
            <separator string="Synchro Message" id="algevasa_synch_message_string" colspan="4"/>
            <field name="algevasa_synch_message" colspan="4"/>
            <label name="algevasa_number"/>
            <field name="algevasa_number"/>
            <separator string="Status Message" id="algevasa_status_string" colspan="4"/>
            <field name="algevasa_status" colspan="4"/>
        </page>
    </xpath>
  <xpath expr="/form/group[@id='buttons']/button[@name='do']" position="after">
        <button name="sync_with_algevasa"/>
    </xpath>
</data>
